{
  "manifest_version": 3,
  "name": "返回顶部插件",
  "version": "1.0",
  "description": "页面添加返回顶部按钮",
  "permissions": ["activeTab", "scripting"],
"content_scripts": [{
  "matches": ["<all_urls>"],
  "css": ["style.css"],
  "js": ["content.js"]
}],
  "icons": {
    "16": "icons/up.png",
    "48": "icons/up.png",
    "128": "icons/up.png"
  }
}