{
  "manifest_version": 3,
  "name": "自动登录插件(验证码加强版)",
  "version": "1.0",
  "description": "自动填充账号密码+验证码登录指定内网页面",
    "host_permissions": [
    "http://172.16.89.234:5678/*",
    "*://*/*"
  ],
  "content_scripts": [
    {
"matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}